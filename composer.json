{
  "name": "iyoramu/banana-php",
  "description": "Adaptable Next-Generation Advanced Nimble Architecture PHP Framework",
  "type": "project",
  "license": "MIT",
  "authors": [
      {
          "name": "IRUTABYOSE Yoramu",
          "email": "yirutabyose@gmail.com"
      }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "firebase/php-jwt": "^6.3",
    "guzzlehttp/guzzle": "^7.5",
    "monolog/monolog": "^3.3",
    "nesbot/carbon": "^2.66",
    "php-di/php-di": "^7.0",
    "predis/predis": "^2.1",
    "ramsey/uuid": "^4.7",
    "symfony/console": "^7.2",
    "symfony/event-dispatcher": "^6.2",
    "symfony/http-foundation": "^6.2",
    "symfony/routing": "^6.2",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
      "phpunit/phpunit": "^10.2",
      "mockery/mockery": "^1.5",
      "phpstan/phpstan": "^1.10",
      "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
        "BananaPHP\\": "app/",
        "App\\": "app/"
    },
    "files": [
        "app/helpers.php",
        "app/Helpers/StringHelper.php",
        "app/Helpers/ArrayHelper.php",
        "app/Helpers/DateHelper.php",
        "app/Helpers/SecurityHelper.php"
    ]
},
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "scripts": {
    "post-autoload-dump": [
        "BananaPHP\\Console\\Kernel::postAutoloadDump"
    ]
},
  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
